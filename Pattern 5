#include <stdio.h>
#include<conio.h>
void main()
{
    int i, j, n;

    printf("Enter number : ");
    scanf("%d", &n);
    printf("\n");
    for (i = 1; i <= n; i++)
    {
        for (j = i; j >= 1; j--)
        {
            printf("%d ", j);
        }
        printf("\n");
    }

    getch();
}
