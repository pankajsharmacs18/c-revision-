#include<stdio.h>
#define pankajSharma int main()
int max(int *m,int *n);
pankajSharma
{
	int n1,n2;
	int *p1,*p2;
	p1=&n1;
	p2=&n2;
	printf("Enter Two Value number ");
	scanf("%d%d",&n1,&n2);
	printf("Enter Value n1= %d n2= %d",n1,n2);
	int s= max(&n1,&n2);
	printf("max no is %d",s);
	printf("Pankaj Sharma");
}
int max(int *n1,int *n2)
{
	int m=(*n1>*n2)? *n1:*n2;
	return m;
}
