#include<stdio.h>
#define pankajSharma int main()
int sum(int *m,int *n);
pankajSharma
{
	int n1,n2;
	int *p1,*p2;
	p1=&n1;
	p2=&n2;
	printf("Enter Two Value number ");
	scanf("%d%d",&n1,&n2);
	printf("Enter Value n1= %d n2= %d",n1,n2);
	int s= sum(&n1,&n2);
	printf("Sum is %d",s);
	printf("Pankaj Sharma");
}
int sum(int *n1,int *n2)
{
	int sum=*n1+*n2;
	return sum;
}
